import java.util.Scanner;
public class Main

{
	public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int x = 0;
        int contaporra = 0;
    
        while(x<101){
            System.out.println(x + " ");
            contaporra = contaporra + x;
            System.out.println(contaporra);
            x++;
        }
    }
}
